{
  "name": "@semihcihan/app-store-connect-api-types",
  "version": "1.0.0",
  "description": "Generated types for App Store Connect API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "codegen": "openapi-typescript openapi.json --output ../src/index.ts",
    "split-openapi": "npx redocly split openapi.json --outDir=openapi"
  },
  "keywords": [
    "app-store",
    "api",
    "types",
    "generated"
  ],
  "author": "Semih Cihan",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "typescript": "^5.8.3",
    "openapi-typescript": "^7.0.2",
    "@redocly/cli": "^1.34.4"
  }
}
