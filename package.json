{
  "name": "@semihcihan/developer-tool",
  "version": "1.0.0",
  "description": "A Node.js CLI tool to synchronize in-app purchases and subscriptions with the App Store from a local JSON file.",
  "main": "dist/cli.js",
  "bin": {
    "app-store-sync": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "codegen": "openapi-typescript openapi.json --output src/generated/app-store-connect-api/index.ts",
    "call": "npx ts-node src/cli.ts fetch --id 6569241842 --file test.json"
  },
  "keywords": [],
  "author": "Focus Tapps",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^17.0.1",
    "jsonwebtoken": "^9.0.2",
    "openapi-fetch": "^0.14.0",
    "yargs": "^17.7.2",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "@types/yargs": "^17.0.33",
    "openapi-typescript": "^7.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
