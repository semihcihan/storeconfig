{
  "name": "storeconfig",
  "version": "0.0.15",
  "description": "CLI tool for App Store Connect synchronization",
  "main": "dist/cli.js",
  "bin": {
    "storeconfig": "dist/cli.js"
  },
  "scripts": {
    "prebuild": "npm run update-readme",
    "predev": "npm run update-readme",
    "build": "node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "dev": "node dist/cli.js",
    "clean": "rm -rf dist",
    "nuke": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "start": "node dist/cli.js",
    "test": "jest --config jest.unit.config.js",
    "test:unit": "jest --config jest.unit.config.js",
    "test:coverage": "jest --config jest.unit.config.js --coverage",
    "prepublishOnly": "npm run build",
    "fetch": "node dist/cli.js fetch --id 6503259293",
    "fetch:all": "node dist/cli.js fetch",
    "apply": "node dist/cli.js apply",
    "plan": "node dist/cli.js plan",
    "apple": "node dist/cli.js apple",
    "validate": "node dist/cli.js validate-format",
    "set-price": "node dist/cli.js set-price",
    "compare-price": "node dist/cli.js compare-price",
    "example": "node dist/cli.js example",
    "user": "node dist/cli.js user",
    "update-readme": "node scripts/generate-readme-example.js",
    "upload-sourcemap": "npx bugsnag-source-maps upload-node --api-key 9214ba2acdaeee4012c236461232c4f3 --overwrite --detect-app-version --bundle dist/cli.js --source-map dist/cli.js.map"
  },
  "keywords": [
    "cli",
    "app-store",
    "ios",
    "synchronization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semihcihan/storeconfig.git"
  },
  "contributors": [
    {
      "name": "Semih Cihan",
      "url": "https://github.com/semihcihan"
    },
    {
      "name": "SelÃ§uk Cihan",
      "url": "https://github.com/selcukcihan"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "homepage": "https://storeconfig.com",
  "files": [
    "dist/",
    "README.md",
    "THIRD_PARTY_LICENSES.md"
  ],
  "dependencies": {
    "@bugsnag/js": "^8.6.0",
    "axios": "^1.10.0",
    "axios-retry": "^4.5.0",
    "dotenv": "^16.6.1",
    "inquirer": "^8.2.5",
    "ora": "^8.2.0",
    "winston": "^3.17.0",
    "yargs": "^17.7.2",
    "zod": "4.1.9",
    "zod-validation-error": "^4.0.2"
  },
  "devDependencies": {
    "@bugsnag/cli": "^3.4.1",
    "@bugsnag/source-maps": "^2.3.3",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/yargs": "^17.0.33",
    "axios-mock-adapter": "^2.1.0",
    "esbuild": "^0.25.11",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
